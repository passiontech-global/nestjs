{
  "name": "@nestjs-meilisearch/meilisearch",
  "version": "1.0.0",
  "description": "A NestJS module for integrating MeiliSearch",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "jest"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.19",
    "meilisearch": "^0.37.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.19",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.1",
    "jest": "^29.7.0",
    "jest-util": "^30.0.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "author": "DTA",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/golevelup/nestjs.git"
  },
  "keywords": [
    "nestjs",
    "meilisearch",
    "search",
    "module"
  ],
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
